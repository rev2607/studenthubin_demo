@model StudentHubMVC.Models.ViewModels.APPolycet

<div class="x_panel" style="border-color: #26b99a; border-width: 2px; ">
    <div class="x_content">
        <h3 style="text-align: center;">AP Polycet Results</h3>
        <div class="row">
            <div class="col-md-6">
                <div class="table-responsive">
                    <table class="table">
                        <tbody>
                            <tr>
                                <td><b>Hall Ticket Number</b></td>
                                <td>@Model.HallTicketNumber</td>
                            </tr>
                            <tr>
                                <td><b>Name</b></td>
                                <td>@Model.FullName</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-6">
                <div class="table-responsive">
                    <table class="table">
                        <tbody>
                            <tr>
                                <td><b>Total</b></td>
                                <td>@Model.Total</td>
                            </tr>
                            <tr>
                                <td><b>Pass Tag</b></td>
                                <td>@Model.Status</td>
                            </tr>
                            <tr>
                                <td><b>Rank</b></td>
                                <td><b style="color: red;">@Model.Rank</b></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-12">
                <div class="table-responsive">
                    <table class="table">
                        <tbody>
                            <tr>
                                <td><b>Maths</b></td>
                                <td>@Model.Maths</td>
                            </tr>
                            <tr>
                                <td><b>Physics</b></td>
                                <td>@Model.Physics</td>
                            </tr>
                            <tr>
                                <td><b>Chemistry</b></td>
                                <td>@Model.Chemistry</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
